{
  "name": "@possible_triangle/tunnel-client",
  "module": "src/cli/main.ts",
  "type": "module",
  "keywords": [
    "tunnel",
    "client",
    "proxy",
    "cli"
  ],
  "exports": {
    ".": "./src/client.ts"
  },
  "bin": {
    "tunnel": "./dist/main.js"
  },
  "files": [
    "dist/**/*.js"
  ],
  "scripts": {
    "dev": "bun run --watch src/cli/main.ts --host localhost:8080",
    "cli": "bun run src/cli/main.ts",
    "build": "bun build src/cli/main.ts --outdir ./dist --target node",
    "gen:config": "bun run script/schema.ts",
    "prepare:publish": "bun run script/publish.ts"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "args": "^5.0.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@possible_triangle/tunnel-contract": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/possible_triangle/Tunnel.git"
  }
}